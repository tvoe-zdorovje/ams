extend type UserMutations {
    createUser(request: CreateUserRequest!): ID!
    updateUser(request: UpdateUserRequest!): ID!
}

input CreateUserRequest {
    password: String!
    firstName: String!
    lastName: String!
    phoneNumber: String!
}

input UpdateUserRequest {
    id: ID!
    firstName: String!
    lastName: String!
    phoneNumber: String!
}
